[general]
status_path = "~/.config/vdirsyncer/status/"

[pair contacts]
a = "contacts_local"
b = "contacts_remote"
collections = ["from a", "from b"]

[storage contacts_local]
type = "filesystem"
path = "/Users/landmaj/Documents/Archive/vdirsyncer"
fileext = ".vcf"

[storage contacts_remote]
type = "carddav"
url = "https://contacts.icloud.com"
username.fetch = ["shell", "security find-generic-password -s 'com.landmaj.vdirsyncer' -a 'username' -w"]
password.fetch = ["shell", "security find-generic-password -s 'com.landmaj.vdirsyncer' -a 'password' -w"]
